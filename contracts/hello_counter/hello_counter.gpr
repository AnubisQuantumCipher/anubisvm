project Hello_Counter is
   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Exec_Dir use "bin";
   for Main use ("hello_counter_main.adb");

   package Compiler is
      for Default_Switches ("Ada") use
         ("-gnatwa", "-gnat2022", "-gnata", "-O2", "-fPIC");
   end Compiler;

   package Prove is
      for Proof_Switches ("Ada") use
         ("--level=2", "--timeout=60", "--proof=per_check");
   end Prove;

end Hello_Counter;
