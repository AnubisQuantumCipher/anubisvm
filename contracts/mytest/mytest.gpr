project Mytest is
   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Exec_Dir use "bin";
   for Main use ("mytest_main.adb");
   package Compiler is
      for Default_Switches ("Ada") use
         ("-gnatwa", "-gnat2022", "-gnata", "-O2");
   end Compiler;
   package Prove is
      for Proof_Switches ("Ada") use ("--level=2");
   end Prove;
end Mytest;
