with "../anubisvm.gpr";

project Test_MLDSA is

   for Source_Dirs use ("src");
   for Object_Dir use "obj";
   for Exec_Dir use "bin";
   for Main use ("test_mldsa.adb");

   package Compiler is
      for Default_Switches ("Ada") use ("-g", "-gnatn", "-O2");
   end Compiler;

   package Binder is
      for Default_Switches ("Ada") use ("-E");
   end Binder;

end Test_MLDSA;
