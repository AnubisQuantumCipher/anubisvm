with "../anubisvm.gpr";

project Test_Shield_Roundtrip is

   for Source_Dirs use ("src/integration");
   for Object_Dir use "obj";
   for Exec_Dir use "bin";
   for Main use ("test_shield_roundtrip.adb");

   package Compiler is
      for Default_Switches ("Ada") use ("-g", "-gnatn", "-O2");
   end Compiler;

   package Binder is
      for Default_Switches ("Ada") use ("-E");
   end Binder;

end Test_Shield_Roundtrip;

